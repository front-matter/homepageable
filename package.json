{
  "name": "notionable",
  "version": "0.2.0",
  "private": true,
  "description": "Notion-powered homepage.",
  "author": "Martin Fenner <martin@front-matter.io>",
  "repository": "front-matter/notionable",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "vercel --prod",
    "deps": "run-s deps:*",
    "deps:update": "[ -z $GITHUB_ACTIONS ] && yarn add notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link": "[ -z $GITHUB_ACTIONS ] && yarn link notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:link on CI'",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "lint": "next lint",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check",
    "posttest": "run-s build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@google-cloud/firestore": "^4.10.1",
    "classnames": "^2.3.1",
    "date-fns": "^2.21.1",
    "eslint": "^7.29.0",
    "got": "^11.8.2",
    "isomorphic-unfetch": "^3.1.0",
    "lqip-modern": "^1.2.0",
    "next": "^11.0.0",
    "next-plausible": "^1.7.0",
    "node-fetch": "^2.6.1",
    "notion-client": "^4.6.4",
    "notion-types": "^4.6.4",
    "notion-utils": "^4.6.4",
    "p-map": "^4.0.0",
    "p-memoize": "^4.0.0",
    "react": "^17.0.2",
    "react-body-classname": "^1.3.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-notion-x": "^4.7.2",
    "react-static-tweets": "^0.5.3",
    "react-use": "^17.2.4",
    "static-tweets": "^0.5.3",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.2.0",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "@types/classnames": "^2.3.1",
    "@types/node": "^15.0.1",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "^17.0.4",
    "autoprefixer": "^10.2.6",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint-config-next": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.4",
    "prettier": "^2.0.5",
    "tailwindcss": "^2.2",
    "typescript": "^4.3.2"
  }
}
